{
    "BlockType" : "dock",
    "Orientation" : "Horizontal",
    "BackgroundImage" : "images/data_bg/stints_bg.png",
    "MarginLeft" : "{MarginLeftTableCell}",
    "PaddingRight": 15,
    "Items" : [
        {
            "BlockType": "itemstack",
            "Spacing": 4,
            "HorizontalAlignment" : "Center",
            "VerticalAlignment" : "Center",
            "ItemStackOptions": {
                "ItemSource": "{Item.Stints}",
                "Template": {
                    "BlockType": "stack",
                    "VerticalAlignment": "Bottom",
                    "MarginRight" : 4,
                    "Spacing": 2,
                    "Items": [
                        {
                            "BlockType": "image",
                            "Height": 40,
                            "VerticalAlignment": "Bottom",
                            "PaddingBottom" : -9,
                            "Source": "tyres_{Item.Tyres}.png"
                        },
                        {
                            "BlockType": "text",
                            "Source": "{Item.Laps}",
                            "HorizontalAlignment": "Center",
                            "VerticalAlignment": "Bottom",
                            "MarginBottom" : -8,
                            "FontSize": 30,
                            "TextOptions": {
                                "FontName": "{FontNameBold}",
                                "HorizontalAlignment": "Center",
                                "Foreground" : "#ffffff"
                            }
                        }

                    ]
                }
            }
        }
    ]
    
}